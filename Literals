public class literals {
    public static void main(String[] args) {
       // byte age=56;//byte literals
        int age=34;//int literals
        char ch='A';//char literals
        float f1=5.6f;//float literals
        double d1=4.66;//double literals,d by default
        boolean a=true;//bool literals
        System.out.println(age);
        String str="Noushad";
        System.out.println(str);
    }
}

//output:34
//noushad
